{
  "bundles": [
    {
      "id": "common:service",
      "outputFolder": "/src/common/services/",
      "templateFolder": "/generator-templates/service/",
      "files": [
        "temp.ts",
        "temp.spec.ts"
      ],
      "data": {
        "di": [
          "$log"
        ],
        "logger": "$log.getInstance('<%= upperCaseName %>', true);"
      }
    },
    {
      "id": "common:component",
      "outputFolder": "/src/common/components/",
      "templateFolder": "/generator-templates/component/",
      "files": [
        "temp.html",
        "temp.scss",
        "temp.ts",
        "temp.spec.ts"
      ],
      "data": {
        "di": [
          "$log"
        ],
        "logger": "$log.getInstance('<%= upperCaseName %>', true);",
        "scssCommonPath": "../../variables"
      }
    },
    {
      "id": "component",
      "outputFolder": "/src/components/",
      "templateFolder": "/generator-templates/component/",
      "files": [
        "temp.html",
        "temp.scss",
        "temp.ts",
        "temp.spec.ts"
      ],
      "data": {
        "di": [
          "$log",
          "AngularServices",
          "AppServices"
        ],
        "logger": "$log.getInstance('<%= upperCaseName %>', true);",
        "scssCommonPath": "../../common/variables"
      }
    },
    {
      "id": "component:view",
      "outputFolder": "/src/components/",
      "templateFolder": "/generator-templates/view-component/",
      "files": [
        "temp.html",
        "temp.scss",
        "temp.ts",
        "temp.spec.ts"
      ],
      "data": {
        "di": [
          "$log",
          "AngularServices",
          "AppServices"
        ],
        "logger": "$log.getInstance('<%= upperCaseName %>', true);",
        "scssCommonPath": "../../common/variables"
      }
    }
  ]
}
